<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alibaba.cloud.youxia.mapper.StorageMapper">
    <update id="deductInventory" parameterType="com.alibaba.cloud.youxia.bo.StorageBo">
    update at_storage t set t.num=t.num-#{num} where t.good_id=#{goodId}
    </update>
</mapper>